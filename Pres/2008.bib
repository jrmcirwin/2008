@article{millerCyclicalityMortalityValue2010,
  title = {Cyclicality, {{Mortality}}, and the {{Value}} of {{Time}}: {{The Case}} of {{Coffee Price Fluctuations}} and {{Child Survival}} in {{Colombia}}},
  shorttitle = {Cyclicality, {{Mortality}}, and the {{Value}} of {{Time}}},
  author = {Miller, Grant and Urdinola, B. Piedad},
  year = {2010},
  month = feb,
  volume = {118},
  pages = {113--155},
  issn = {0022-3808},
  abstract = {Recent studies demonstrate procyclical mortality in wealthy countries, but there are reasons to expect a countercyclical relationship in developing nations. We investigate how child survival in Colombia responds to fluctuations in world Arabica coffee prices \textendash{} and document starkly procyclical child deaths. In studying this result's behavioral underpinnings, we highlight that: (1) The leading determinants of child health are inexpensive but require considerable time, and (2) As the value of time declines with falling coffee prices, so does the relative price of health. We find a variety of direct evidence consistent with the primacy of time in child health production.},
  journal = {The journal of political economy},
  number = {1},
  pmcid = {PMC3214991},
  pmid = {22090662}
}

